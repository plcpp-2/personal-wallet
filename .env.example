# Network Configuration
NETWORK=mainnet # mainnet, goerli, polygon

# Network RPC URLs
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Provider API Keys (use any/all you have)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
ALCHEMY_KEY=your-alchemy-key
INFURA_KEY=your-infura-key
TENDERLY_API_KEY=your_tenderly_api_key
TENDERLY_KEY=your-tenderly-key
CUSTOM_RPC=your-custom-rpc-url
BASESCAN_API_KEY=your_basescan_api_key

# Default Provider (alchemy, infura, tenderly, or custom)
DEFAULT_PROVIDER=alchemy

# Wallet Configuration
NEXT_PUBLIC_WALLET_ADDRESS=your_wallet_address_here
PRIVATE_KEY=your_private_key

# Coinbase Configuration
NEXT_PUBLIC_COINBASE_API_KEY=your_coinbase_api_key_here
NEXT_PUBLIC_COINBASE_API_SECRET=your_coinbase_api_secret_here
COINBASE_WALLET_ADDRESS=your_coinbase_wallet_address

# Development Settings
NODE_ENV=development # development, production
ENABLE_CHEATCALLS=true # true/false

# Explorer API Keys (optional)
ETHERSCAN_API_KEY=your-etherscan-key

# Impersonation Settings (for development)
IMPERSONATE_ADDRESS=
FORK_BLOCK_NUMBER=

# Deployment
VERCEL_TOKEN=your_vercel_token
VERCEL_ORG_ID=your_vercel_org_id
VERCEL_PROJECT_ID=your_vercel_project_id

# Gas Settings
GAS_PRICE_MULTIPLIER=1.1
MAX_PRIORITY_FEE=2
